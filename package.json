{
  "name": "hello-world-react-component-library",
  "version": "0.0.6",
  "license": "MIT",
  "description": "A Collection of Components for Telling the World Hello.",
  "homepage": "https://unfoldingword-box3.github.io/hello-world-react-component-library/",
  "repository": {
    "type": "git",
    "url": "https://github.com/unfoldingWord-box3/hello-world-react-component-library"
  },
  "private": false,
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "predeploy": "yarn styleguide:build",
    "deploy": "gh-pages -d styleguide",
    "postpublish": "yarn deploy",
    "cypress:open": "cypress open",
    "test": "cross-env NODE_ENV=test nyc --exclude-after-remap=false start-test 6060 cypress:open && nyc report --reporter=json-summary",
    "build": "react-scripts build",
    "prepublishOnly": "rm -fr ./dist & babel ./src --out-dir ./dist -s inline"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": "7.7.2",
    "@cypress/code-coverage": "1.10.2",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "@types/cypress": "1.1.3",
    "@types/jest": "24.0.22",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "@unfoldingword/eslint-config": "1.0.11",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "coveralls": "3.0.7",
    "cross-env": "^6.0.3",
    "cypress": "3.6.1",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-chai-friendly": "0.5.0",
    "eslint-plugin-cypress": "2.7.0",
    "eslint-plugin-mdx": "1.0.1",
    "eslint-plugin-react": "7.12.4",
    "gh-pages": "^2.0.1",
    "istanbul-lib-coverage": "2.0.5",
    "lodash": "^4.17.11",
    "nyc": "14.1.1",
    "path": "latest",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-scripts": "3.2.0",
    "react-styleguidist": "10.2.1",
    "source-map-support": "0.5.16",
    "start-server-and-test": "1.10.6",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "resolutions": {
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "excludeAfterRemap": false,
    "exclude": [
      "**/*.spec.js"
    ],
    "include": [
      "**/components/**"
    ]
  }
}
